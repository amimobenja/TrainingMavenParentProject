<?xml version="1.0" encoding="UTF-8"?>
<api context="/filterrestapi" name="FilterRestAPI" xmlns="http://ws.apache.org/ns/synapse">
  <resource methods="GET" protocol="http" uri-template="/comments/{company}">
    <inSequence>
      <sequence key="FilterRestAPISequence"/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
  </resource>
  <resource methods="GET" protocol="http" uri-template="/switch_comments/{company}">
    <inSequence>
      <sequence key="SwitchSequence"/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
  </resource>
</api>
